        "dark": {
          "default": "#FF999999",
          "subtle": "#99999999"
        },
        "light": {
          "default": "#FFFFFFFF",
          "subtle": "#99FFFFFF"
        },
        "accent": {
          "default": "#FF2E89FC",
          "subtle": "#CC2E89FC"
        },
        "good": {
          "default": "#CC00FF00",
          "subtle": "#9900FF00"
        },
        "warning": {
          "default": "#CCFF9800",
          "subtle": "#99FF9800"
        },
        "attention": {
          "default": "#CCFF0000",
          "subtle": "#99FF0000"
        }
      },
      "backgroundColor": "#33FFFFFF"
