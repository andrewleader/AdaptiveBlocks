{
  "choiceSetInputValueSeparator": ",",
  "supportsInteractivity": true,
  "fontFamily": "Segoe UI",
  "spacing": {
    "small": 3,
    "default": 8,
    "medium": 20,
    "large": 30,
    "extraLarge": 40,
    "padding": 20
  },
  "separator": {
    "lineThickness": 1,
    "lineColor": "#FFEEEEEE"
  },
  "fontSizes": {
    "small": 12,
    "default": 14,
    "medium": 17,
    "large": 21,
    "extraLarge": 26
  },
  "fontWeights": {
    "lighter": 200,
    "default": 400,
    "bolder": 600
  },
  "imageSizes": {
    "small": 40,
    "medium": 80,
    "large": 160
  },
  "containerStyles": {
    "default": {
      "foregroundColors": {
        "default": {
          "default": "#FF000000",
          "subtle": "#EE333333"
        },
        "dark": {
          "default": "#FF000000",
          "subtle": "#66000000"
        },
        "light": {
          "default": "#FFFFFFFF",
          "subtle": "#33000000"
        },
        "accent": {
          "default": "#FF2E89FC",
          "subtle": "#882E89FC"
        },
        "good": {
          "default": "#FF54a254",
          "subtle": "#DD54a254"
        },
        "warning": {
          "default": "#FFe69500",
          "subtle": "#DDe69500"
        },
        "attention": {
          "default": "#FFcc3300",
          "subtle": "#DDcc3300"
        }
      },
      "backgroundColor": "#00000000"
    },
    "emphasis": {
      "foregroundColors": {
        "default": {
          "default": "#FF000000",
          "subtle": "#EE333333"
        },
        "dark": {
          "default": "#FF000000",
          "subtle": "#66000000"
        },
        "light": {
          "default": "#FFFFFFFF",
          "subtle": "#33000000"
        },
        "accent": {
          "default": "#FF2E89FC",
          "subtle": "#882E89FC"
        },
        "good": {
          "default": "#FF54a254",
          "subtle": "#DD54a254"
        },
        "warning": {
          "default": "#FFe69500",
          "subtle": "#DDe69500"
        },
        "attention": {
          "default": "#FFcc3300",
          "subtle": "#DDcc3300"
        }
      },
      "backgroundColor": "#08000000"
    }
  },
  "actions": {
    "maxActions": 5,
    "spacing": "Default",
    "buttonSpacing": 10,
    "showCard": {
      "actionMode": "Inline",
      "inlineTopMargin": 16,
      "style": "emphasis"
    },
    "preExpandSingleShowCardAction": false,
    "actionsOrientation": "Horizontal",
    "actionAlignment": "Stretch"
  },
  "adaptiveCard": {
    "allowCustomStyle": false
  },
  "imageSet": {
    "imageSize": "Medium",
    "maxImageHeight": 100
  },
  "media": {
    "allowInlinePlayback": true
  },
  "factSet": {
    "title": {
      "size": "Default",
      "color": "Default",
      "isSubtle": false,
      "weight": "Bolder",
      "wrap": true
    },
    "value": {
      "size": "Default",
      "color": "Default",
      "isSubtle": false,
      "weight": "Default",
      "wrap": true
    },
    "spacing": 10
  },
  "cssClassNamePrefix": null
